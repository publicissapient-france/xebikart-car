name: xebikart

docker_env:
    image: xebikart_ml

entry_points:
    main:
        command: "python execute_and_log_notebook.py"
    vae:
        command: "python execute_and_log_notebook.py --input workspace/auto_encoder/variational_auto_encoder.ipynb --output output_variational_auto_encoder.ipynb"
    reinforcement_learning:
        command: "python execute_and_log_notebook.py --start-xvfb --input workspace/reinforcement_learning/soft_actor_critic.ipynb --output output_soft_actor_critic.ipynb"
    cnn:
        command: "python execute_and_log_notebook.py --input workspace/supervised_models/CNN.ipynb --output output_cnn.ipynb"

