        command: "python execute_and_log_notebook.py --start-xvfb --input workspace/reinforcement_learning/sac_reward_cte_on_track.ipynb --output output_sac_reward_cte_on_track.ipynb"
    cnn:
        command: "python execute_and_log_notebook.py --input workspace/supervised_models/CNN.ipynb --output output_cnn.ipynb"

