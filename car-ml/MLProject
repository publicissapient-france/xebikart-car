    reinforcement_learning:
        command: "python execute_and_log_notebook.py --start-xvfb --input workspace/reinforcement_learning/soft_actor_critic.ipynb --output output_soft_actor_critic.ipynb"
    rl_sac_steering:
        command: "python execute_and_log_notebook.py --start-xvfb --input workspace/reinforcement_learning/sac_steering_spaces_only.ipynb --output output_sac_steering_spaces_only.ipynb"
